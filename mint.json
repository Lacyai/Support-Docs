{
  "name": "Alex Sidebar",
  "$schema": "https://mintlify.com/schema.json",
  "theme": "prism",
  "logo": {
    "dark": "/logo/alex_logo.png",
    "light": "/logo/alex_logo.png"
  },
  "layout": "sidenav",
  "favicon": "/logo/alex_logo.png",
  "colors": {
    "primary": "#6ED511",
    "light": "#6ED511",
    "dark": "#6ED511"
  },
  "font": {
    "headings": {
      "family": "Inter"
    },
    "body": {
      "family": "Open Sans"
    }
  },
  "sidebar": {
    "items": "border"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:daniel@alexcodes.app"
    }
  ],
  "rounded": "default",
  "topbar": {
    "style": "default"
  },
  "search": {
    "location": "top"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "topbarCtaButton": {
    "type": "link",
    "style": "roundedRectangle",
    "arrow": true,
    "name": "Discord Community",
    "url": "https://discord.gg/T5zxfReEnd"
  },
  "navigation": [
    {
      "group": "Alex Sidebar",
      "icon": "rocket",
      "iconType": "solid",
      "pages": [
        "getting-started/core-concepts",
        "get-started/getting-started",
        "get-started/cheatsheet",
        "get-started/data-usage"
      ]
    },
    {
      "group": "Chat",
      "icon": "message",
      "iconType": "solid",
      "pages": [
        "chat/overview",
        "chat/tabs",
        "chat/context/agents",
        "chat/context/memory",
        "chat/context/commands",
        "chat/input-modes/image-to-code",
        "chat/input-modes/voice-mode",
        "chat/ui/customization",
        "chat/best-practices"
      ]
    },
    {
      "group": "Working with Search",
      "icon": "magnifying-glass",
      "iconType": "solid",
      "pages": [
        "search/advanced-search"
      ]
    },
    {
      "group": "Working with Completions",
      "icon": "sparkles",
      "iconType": "solid",
      "pages": [
        "completions/inline-suggestions",
        "completions/error-fixes"
      ]
    },
    {
      "group": "Configuration",
      "icon": "cog",
      "iconType": "solid",
      "pages": [
        "configuration/model-configuration",
        {
          "group": "Keyboard Shortcuts",
          "pages": [
            "configuration/code-context-shortcuts",
            "configuration/navigation-shortcuts"
          ]
        },
        "configuration/git-integration",
        "configuration/custom-prompts",
        "configuration/codebase-indexing"
      ]
    },
    {
      "group": "Window Settings",
      "icon": "window",
      "iconType": "solid",
      "pages": [
        "windows/overview",
        "windows/activation",
        "windows/positioning",
        "windows/interface-hints"
      ]
    },
    {
      "group": "Support",
      "icon": "life-ring",
      "iconType": "solid",
      "pages": [
        "support/getting-help",
        "support/discord-channels",
        "support/troubleshooting"
      ]
    }
  ],
  "footer": {
    "socials": {
      "x": "https://x.com/alexcodes_ai",
      "discord": "https://discord.gg/T5zxfReEnd",
      "linkedin": "https://www.linkedin.com/company/alex-sidebar/"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  }
}
